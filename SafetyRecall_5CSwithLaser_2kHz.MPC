S.S.1,   \Set Inter-tone/laser time variables from List Z, this will assign different times within the ListZ for each run of the program
S1,
    0.001": RANDD E = Z; ON E ---> S2
S2,
    0.005": OFF E ---> S3

S3,
    0.001": RANDD F = Z; ON F ---> S4

S4,
    0.005": OFF F  ---> S5
S5,
    0.001": RANDD G = Z; ON G ---> S6

S6,
    0.005": OFF G   ---> S7

S7,
    0.001": RANDD H = Z; ON H ---> S8

S8,
     0.005": OFF H ---> S9
S9,
    0.001": RANDD I = Z; ON I  ---> S10

S10,
   0.005": OFF I ---> S11

S11,
    0.001": RANDD K = Z; ON K  ---> S12

S12,
     0.005": OFF K  ---> SX


S.S.2,    \set variables, Initiate 5 CS
S1,
    #START: SET A = 2000, B = 85, J = 20 ---> S2

S2,
    0.001": SET E = E * 1", F = F * 1", G = G * 1", H = H * 1", I = I * 1", J = J * 1", K = K * 1", S = S * 1" ---> S3

S3,    \initialize Sound Generator
    0.001": ~InitANL926;~;
            ~SetFreq(MG, BOX, A);~;
            ~SetAmp(MG, BOX, B);~;
            ~SetDur(MG, BOX, J);~;
              ---> S4

 \1st CS Presentation~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
S4,      \ Turn on Laser after E seconds; 5sec later turn on CS/tone
       E#T:  ON ^Laser ---> S5

S5,
        5": ~ToneOn (MG, BOX);~;
            ON ^CS Tone ---> S6

S6,      \Turn off tone & laser simultaneously
       J#T: ~ToneOff(MG, BOX);~;
            OFF ^CS Tone, ^Laser ---> S7

\2nd CS Presentation~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

S7,
       F#T:  ON^Laser ---> S8

S8,
        5":  ~ToneOn (MG, BOX);~;
             ON ^CS Tone ---> S9

S9,
       J#T: ~ToneOff(MG, BOX);~;
            OFF ^CS Tone, ^Laser ---> S10
\3rd CS Presentation~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

S10,
       G#T:  ON ^Laser ---> S11

S11,
        5": ~ToneOn (MG, BOX);~;
             ON ^CS Tone ---> S12

S12,
       J#T: ~ToneOff(MG, BOX);~;
            OFF ^CS Tone, ^Laser  ---> S13

\4th CS Presentation~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

S13,
        H#T:  ON ^Laser ---> S14

S14,
        5":   ~ToneOn (MG, BOX);~;
               ON ^CS Tone ---> S15

S15,
       J#T: ~ToneOff(MG, BOX);~;
            OFF ^CS Tone, ^Laser ---> S16

\5th CS Presentation~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

S16,
        I#T:  ON ^Laser ---> S17


S17,
        5": ~ToneOn (MG, BOX);~;
               ON ^CS Tone ---> S18

S18,
       J#T: ~ToneOff(MG, BOX);~;
            OFF ^CS Tone, ^Laser ---> S19

S19,      \ End the Session K seconds after the Shock and Tone finish
       30":  ---> StopAbortFlush

