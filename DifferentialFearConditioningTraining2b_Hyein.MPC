S17,                                                                                    \seven CS plus
       1": ADD J;
       IF J < 30 [@True, @False]
            @True: ~ONFreq(MG, BOX, A [1]);~;
                    ON ^CS Tone ---> S17
            @False: IF J = 30 [@True]
                    @True: ~ONFreq(MG, BOX, A [1]);~;
                    ON ^GridFloor, ^CS Tone ---> S18

S18,
   1": OFF ^GridFloor, ^CS Tone; RANDI B = Z ---> S19

S19,
    B#T: SHOW 36, 8th ISI (sec), B/100 ---> S20


S20,
    1": ADD K;                                                                              \eight CS plus
        IF K < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A [1]);~;
                    ON ^CS Tone ---> S20
             @False: IF K = 30 [@True]
                     @True: ~ONFreq(MG, BOX, A [1]);~;
                     ON ^GridFloor, ^CS Tone ---> S21

S21,
    1": OFF ^GridFloor, ^CS Tone; RANDI B = Z  ---> S22

S22,
    B#T: SHOW 41, 9th ISI (sec), B/100 ---> S23

S23,                                                                                          \nine CS minus
     1": ADD L;
         IF L <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[0]);~;
                     ON ^CS Tone---> S23
             @False: OFF ^CS Tone; RANDI B = Z ---> S24

S24,
    B#T: SHOW 46, 10th ISI (sec), B/100 ---> S25

S25,
     1": ADD M;                                                                              \ten CS plus
         IF M < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[1]);~;
                     ON ^CS Tone ---> S25
             @False: IF M = 30 [@True]
                     @True: ~ONFreq(MG, Box, A[1]);~;
                     ON ^GridFloor, ^CS Tone ---> S26

S26,
    1": OFF ^GridFloor, ^CS Tone; RANDI B = Z ---> S27

S27,
    B#T: SHOW 51, 11th ISI (sec), B/100 ---> S28

S28,                                                                                          \eleven CS plus
    1": ADD N;
        IF N < 30 [@True, @False]
            @True: ~ONFreq(MG, BOX, A[1]);~;
                   ON ^CS Tone---> S28
            @False: IF N = 30 [@True]
                    @True: ~ONFreq(MG, BOX, A [1]);~;
                    ON ^GridFloor, ^CS Tone ---> S29

S29,
    1": OFF ^GridFloor, ^CS Tone; RANDI B = Z ---> S30

S30,
    B#T: SHOW 56, 12th ISI (sec), B/100 ---> S31

S31,
    1": ADD O;                                                                              \twelve CS minus
         IF O <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[0]);~;
                     ON ^CS Tone---> S31
             @False: OFF ^CS Tone; RANDI B = Z ---> S32

S32,
    B#T: SHOW 61, END (sec), B/100 ---> STOPABORTFLUSH

S.S.3,
S1,
    #START: ---> S2

S2,
    1": ADD P; SHOW 2, Session Time, P ---> S2

    S.S.4
