\Differential Fear Conditioning Training 2
\September 12, 2016

\Differential Fear Conditioning Training 2.mpc

\12 trials, CS+/CS-, CS+ co-delivered 1 sec shock
\CS+: 7kHzpips, 30 sec of pips; CS-: 1kHz pips, 30 sec of pips
\interstimulus intervals of 60, 90, 120 seconds from List Z


\Outputs
^CS Tone = 6
^HouseLight = 7
^GridFloor  = 8

^FreqOne    = 0
^FreqSeven  = 1
^Amp        = 2
^RF         = 3
^Dur        = 4

DIM A = 4



\Inter stimulus (CS) intervals
List Z = 60", 90", 120"



S.S.1,
S1,
    0.001": SET A(^FreqOne) = 1000, A(^FreqSeven) = 7000, A(^Amp)= 100, A(^RF) = 2, A(^Dur) = 46 ---> SX



S.S.2,       \ 6 CS+, 6 CS- protocol

S1,
          #START: ~InitANL926;~;                    \resetting ANL926
                  ~SetAMP(MG, BOX, A [2]);~;        \initialze amplitude (dB)
                  ~SetRF(MG, BOX, A [3]);~;         \set Rise and Fall  (msec)
                  ~SetDur(MG,BOX, A [4]);~;         \set duration of tone (msec)
                  RANDI B = Z ---> S2


S2, \60, 90, 120 sec from List Z
     B#T: SHOW 1, 1st ISI (sec), B/100 ---> S3

S3,                                                                                         \one CS minus
        1": ADD C;
        IF C <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A [0]);~;
                 ON ^CS Tone ---> S3
             @False: OFF ^CS Tone; RANDI B = Z ---> S4

S4,
    B#T: SHOW 6, 2nd ISI (sec), B/100 ---> S5

S5,
  1": ADD D;
        IF D < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[1]);~;
                     ON ^CS Tone ---> S5                                      \two CS plus
             @False: IF D = 30 [@True]
                     @True: ~ONFreq(MG, BOX, A[1]);~;
                     ON ^GridFloor, ^CS Tone ---> S6

S6,
    1": OFF ^GridFloor, ^CS Tone; RANDI B = Z  ---> S7

S7,
    B#T: SHOW 11, 3rd ISI (sec), B/100 ---> S8

S8,                                                                                        \ Three CS minus
    1": ADD E;
        IF E <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A [0]);~;
                     ON ^CS Tone ---> S8
             @False: OFF ^CS Tone; RANDI B = Z ---> S9

S9,
     B#T: SHOW 16, 4th ISI (sec), B/100 ---> S10

S10,                                                                                     \ Four CS plus
1": ADD F;
        IF F < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A [1]);~;
                     ON ^CS Tone ---> S10
             @False: IF F = 30 [@True]
                     @True: ~ONFreq(MG, BOX, A [1]);~;
                     ON ^GridFloor, ^CS Tone ---> S11

S11,
    1": OFF ^GridFloor, ^CS Tone; RANDI B = Z ---> S12

S12,
    B#T: SHOW 21, 5th ISI (sec), B/100 ---> S13

S13,                                                                                   \ Five CS minus
    1": ADD H;
        IF H <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[0]);~;
                     ON ^CS Tone ---> S13
             @False: OFF ^CS Tone; RANDI B = Z ---> S14

S14,
    B#T: SHOW 26, 6th ISI (sec), B/100 ---> S15

S15,                                                                                     \six CS minus
    1": ADD I;
        IF I <= 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A [0]);~;
                    ON ^CS Tone---> S15
             @False: OFF ^CS Tone; RANDI B = Z ---> S16

S16,
