S27,                                                                                          \eleven CS plus
   1": ON ^Laser; ADD N;
         IF N < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[1]);~;
                     ON ^CS Tone ---> S27
             @False: IF N = 30 [@True]
                    @True: ~ONFreq(MG, BOX, A [1]);~;
                    ON ^GridFloor, ^CS Tone ---> S28
S28,
    1": OFF ^GridFloor, ^CS Tone, ^Laser; RANDI B = Z  ---> S29

S29,
    B#T: SHOW 56, 12th ISI (sec), B/100 ---> S30

S30,                                                                                          \twelve CS plus
        1": ON ^Laser; ADD O;
         IF O < 30 [@True, @False]
             @True: ~ONFreq(MG, BOX, A[1]);~;
                     ON ^CS Tone ---> S30
             @False: IF O = 30 [@True]
                    @True: ~ONFreq(MG, BOX, A [1]);~;
